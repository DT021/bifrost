{
  "version": "2",
  "name": "Bifrost api gateway",
  "port": 8000,
  "in_cluster": false,
  "override_service_address": "http://kube-worker1:30293/",
  "log_level": "DEBUG",
  "metrics": {
    "enabled": true,
    "collection_time": "60s",
    "proxy_disabled": false,
    "router_disabled": false,
    "backend_disabled": false,
    "endpoint_disabled": false,
    "listen_address": "8090"
  },
  "upstream_path_prefix": "/api",
  "downstream_path_prefix": "",
  "endpoints": [
    {
      "upstream_path": "",
      "upstream_path_prefix": "/api",
      "downstream_path_prefix": "/leasing/v1",
      "service_name": "lsng-api",
      "handler_type": "event",
      "topic": ""
    },
    {
      "upstream_path_prefix": "/downstream-api-2",
      "methods": [
        "POST",
        "GET"
      ],
      "service_name": "downstream-api-2"
    },
    {
      "upstream_path_prefix": "/downstream-api-1",
      "methods": [
        "POST",
        "GET"
      ],
      "service_name": "downstream-api-1"
    }
  ],
  "handlers": {
    "nats": {
      "nats_url": "nats://kube-worker1:31291",
      "cluster": "faas-cluster",
      "client_id": "GoGatewayClientId",
      "q_group": "GoGateway",
      "durable_name": "durable",
      "topic_prefix": "ch.Commands.blalb."
    }
  }
}